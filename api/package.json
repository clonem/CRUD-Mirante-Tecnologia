{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "rimraf dist && nest start",
    "start:dev": "rimraf dist && nest start --watch",
    "start:debug": "rimraf dist && nest start --debug --watch",
    "start:prod": "rimraf dist && node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:build": "tsc -p tsconfig.json",
    "test:default": "./node_modules/.bin/mocha --no-timeouts --colors \"./dist/**/*.spec.js\"",
    "test:windows": "run-s prebuild test:build test:default",
    "test:other": "tsc -p tsconfig.json && ./node_modules/.bin/mocha --no-timeouts --colors \"./dist/**/*.spec.js\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix --quiet",
      "git add"
    ]
  },
  "dependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/swagger": "^4.5.9",
    "@nestjs/typeorm": "^7.1.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.25",
    "typeorm-transactional-cls-hooked": "^0.1.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.2",
    "@types/express": "^4.17.3",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.9.1",
    "@types/supertest": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-sonarjs": "^0.5.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.9",
    "mocha": "^10.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "rosie": "^2.0.1",
    "supertest": "^4.0.2",
    "ts-jest": "25.2.1",
    "ts-loader": "^6.2.1",
    "ts-mockito": "^2.5.0",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  }
}
